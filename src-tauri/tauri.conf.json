{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Trac nghiem chinh tri",
  "version": "1.0.0",
  "identifier": "com.newtun.tracnghiemchinhtri",
  "build": {
    "frontendDist": "../dist/Trac-Nghiem-Chinh-Tri/browser",
    "devUrl": "http://localhost:4200",
    "beforeDevCommand": "ng s",
    "beforeBuildCommand": "ng build"
  },
  "app": {
    "windows": [
      {
        "title": "Trac nghiem chinh tri",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "center": true,
        "maximized": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
